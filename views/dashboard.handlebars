{{#each userPosts as |userPost| }}
<div class="card">
  <div class="card-header">
    {{userPost.title}} Post
    <a class="nav-link" href="/dashboard">View</a>
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p>{{userPost.body}}</p>
      <footer class="blockquote-footer">{{userPost.updatedAt}}</footer>
    </blockquote>
  </div>
</div>
{{/each}}